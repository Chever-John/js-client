# Ratel 客户端环境变量配置
# 复制此文件为 .env 并根据需要修改配置

# 服务器主机名或IP地址
RATEL_SERVER_HOST=ratel-be.youdomain.com

# 服务器端口号
RATEL_SERVER_PORT=80

# 服务器名称
RATEL_SERVER_NAME=Nico

# 服务器版本
RATEL_SERVER_VERSION=v1.3.0

# 是否为开发环境
RATEL_IS_DEVELOPMENT=false

# 使用示例：
# 开发环境配置
# RATEL_SERVER_HOST=localhost
# RATEL_SERVER_PORT=1025
# RATEL_IS_DEVELOPMENT=true

# 生产环境配置
# RATEL_SERVER_HOST=game.example.com
# RATEL_SERVER_PORT=80
# RATEL_IS_DEVELOPMENT=false 